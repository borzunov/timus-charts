{
    "manifest_version": 2,
    "name": "Timus Charts",
    "description": "Adds charts to Timus Online Judge's profiles",
    "author": "Alexander Borzunov",
    "version": "1.0",
    "icons": {
        "128": "icon.png"
    },
    "content_scripts": [{
        "js": [
            "jquery.min.js",
            "jquery.jqplot.min.js",
            "jqplot.dateAxisRenderer.min.js",
            "timus.user.js",
            "spin.min.js"
        ],
        "include_globs": [
            "http://acm.timus.ru/author.aspx*"
        ],
        "exclude_globs": [],
        "matches": [
            "http://acm.timus.ru/author.aspx*"
        ],
        "run_at": "document_end"
    }]
}
